@model IEnumerable<Ictshop.Models.Nhacungcap>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Quản lý Nhà cung cấp</h2>

<p>
    @Html.ActionLink("Thêm nhà cung cấp", "Create")
</p>
<table class="table">
    <tr>

        <th>
            Mã Nhà Cung Cấp
        </th>
        <th> Tên Nhà Cung Cấp
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Mancc)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tenncc)
    </td>
    <td>
        @Html.ActionLink("Sửa", "Edit", new { id = item.Mancc }) |
        @Html.ActionLink("Chi tiết", "Details", new { id = item.Mancc }) |
        @Html.ActionLink("Xoá", "Delete", new { id = item.Mancc })
    </td>
</tr>
}

</table>
